{
  "name": "grow",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/engines",
      "prisma",
      "esbuild",
      "sharp"
    ]
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev --webpack",
    "dev:turbo": "next dev --turbopack",
    "dev:pwa": "ENABLE_PWA=true next dev --webpack",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint",
    "db:seed": "prisma db seed",
    "db:generate": "prisma generate",
    "db:migrate:dev": "prisma migrate dev ",
    "db:reset": "prisma migrate reset",
    "postinstall": "prisma generate",
    "vercel-build": "prisma generate && prisma migrate deploy && next build --webpack",
    "db:migrate:customers": "tsx scripts/migrateCustomers.ts",
    "db:migrate:plants": "tsx scripts/migratePlants.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@react-pdf/renderer": "^4.3.2",
    "@serwist/next": "^9.5.0",
    "bcryptjs": "^3.0.3",
    "luxon": "^3.7.2",
    "next": "16.1.1",
    "next-auth": "5.0.0-beta.30",
    "pg": "^8.16.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.69.0",
    "recharts": "^3.6.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@tailwindcss/postcss": "^4",
    "@types/luxon": "^3.7.1",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "daisyui": "^5.5.14",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "prisma": "^7.2.0",
    "serwist": "^9.5.0",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
